{
    "name": "collinped/aimtell-php",
    "description": "PHP SDK for Aimtell",
    "keywords": [
        "collinped",
        "aimtell-php",
        "aimtell"
    ],
    "homepage": "https://github.com/collinped/aimtell-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Collin Pedersen",
            "email": "me@collinped.com",
            "homepage": "https://www.collinped.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Collinped\\Aimtell\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Collinped\\Aimtell\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/collinped"
        }
    ]
}
